<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SampleStudent</display-name>
  <welcome-file-list>
    <welcome-file>Login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>StudentController</servlet-name>
    <servlet-class>com.pack.controller.StudentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentController</servlet-name>
    <url-pattern>/StudentController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewStudentController</servlet-name>
    <servlet-class>com.pack.controller.ViewStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewStudentController</servlet-name>
    <url-pattern>/ViewStudentController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RemoveStudentController</servlet-name>
    <servlet-class>com.pack.controller.RemoveStudentController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RemoveStudentController</servlet-name>
    <url-pattern>/RemoveStudentController</url-pattern>
  </servlet-mapping>
  <error-page>
  <exception-type>java.lang.ClassNotFoundException</exception-type>
  <location>/error.jsp</location>
  </error-page>
</web-app>